stock SendAdminMessage(color,const astring[])
{
	foreach(new i : Player)
	{
		if(GetPlayerState(i) != PLAYER_STATE_NONE)
		{
			if(IsPlayerAdmin(i))
			{
				SendClientMessage(i, color, astring);
			}
		}
	}
	return 1;
}

stock GetName(playerid)
{
	new pName[24];
    GetPlayerName(playerid, (pName), sizeof(pName));
    return pName;
}

public OnPlayerText(playerid, text[])
{
   	new placeholder;
	for(new i = 0; i < sizeof(BadWords); i++)
	{
	    placeholder = strfind(text,BadWords[i],true);
        if(placeholder != -1)
	    {
	        for(new x = placeholder; x < placeholder + strlen(BadWords[i]); x++)
	        {
	            text[x] = '*';
	        }
	    }
	}
	if(MobilePlayer[playerid][calling] == 1)
	{
        new str[158], astr[200];
        format(str, sizeof(str), "%d (%s): %s", MobilePlayer[playerid][number], GetName(playerid),text);
        SendClientMessage(MobilePlayer[playerid][caller], 0xBFC0C2FF, str);
        SendClientMessage(playerid, 0xBFC0C2FF, str);
        format(astr, sizeof(astr),"PHONE: %s [%d][%d] to %s [%d][%d]: %s",GetName(playerid),playerid,MobilePlayer[playerid][number],GetName(MobilePlayer[playerid][caller]),MobilePlayer[playerid][caller],MobilePlayer[MobilePlayer[playerid][caller]][number],text);
        SendAdminMessage(0xAFAFAFAA,astr);
        return 0;
    }
    else if(MobilePlayer[playerid][calling] == 3)
    {
        new str[158], astr[200];
        format(str, sizeof(str), "New message of %d (%s) MESSAGE:", MobilePlayer[playerid][number], GetName(playerid));
		new str2[158];
		format(str2,sizeof(str2), "%s",text);
        SendClientMessage(MobilePlayer[playerid][caller], 0xBFC0C2FF, str);
        SendClientMessage(MobilePlayer[playerid][caller], 0xBFC0C2FF, str2);
        format(astr, sizeof(astr),"PHONE: %s [%d][%d] to %s [%d][%d]: %s",GetName(playerid),playerid,MobilePlayer[playerid][number],GetName(MobilePlayer[playerid][caller]),MobilePlayer[playerid][caller],MobilePlayer[MobilePlayer[playerid][caller]][number],text);
        SendAdminMessage(0xAFAFAFAA,astr);
        SendClientMessage(playerid, 0xBFC0C2FF, "Message sent!");
        KillTimer(TimerWaitCalling[playerid]);
        MobilePlayer[playerid][calling] = 0;
        return 0;
	}
    return 1;
}

CMD:myphone(playerid,params[])
{
    SendClientMessage(playerid, ORANGE, "Phone Commands: /phone , /mynumber , /number , /answer , /reject , /sel ");
    return 1;
}

CMD:sel(playerid,params[])
{
	    ActualizarTiempo();
	    GetPlayerZone(playerid);
	    if(IsPlayerInArea(playerid,44.60,-2892.90,2997.00,-768.00))PlayerTextDrawSetString(playerid,Galaxy3StartMenu2,"3G"); //LS
	    if(IsPlayerInArea(playerid,869.40,596.30,2997.00,2993.80)) PlayerTextDrawSetString(playerid,Galaxy3StartMenu2,"3G");//LV
	    if(IsPlayerInArea(playerid,-480.50,596.30,869.40,2993.80))PlayerTextDrawSetString(playerid,Galaxy3StartMenu2,"G");//BoneCounty
	    if(IsPlayerInArea(playerid,-2997.40,1659.60,-480.50,2993.80))PlayerTextDrawSetString(playerid,Galaxy3StartMenu2,"E");//TierraRobada
	    if(IsPlayerInArea(playerid,-1213.90,596.30,-480.50,1659.60))PlayerTextDrawSetString(playerid,Galaxy3StartMenu2,"G");//TierraRobada
	    if(IsPlayerInArea(playerid,-2997.40,-1115.50,-1213.90,1659.60))PlayerTextDrawSetString(playerid,Galaxy3StartMenu2,"3G");//SanFierro
	    if(IsPlayerInArea(playerid,-1213.90,-768.00,2997.00,596.30))PlayerTextDrawSetString(playerid,Galaxy3StartMenu2,"G");//RedCountry
	    if(IsPlayerInArea(playerid,-1213.90,-2892.90,44.60,-768.00))PlayerTextDrawSetString(playerid,Galaxy3StartMenu2,"E");//FlintCounty
	    if(IsPlayerInArea(playerid,-2997.40,-2892.90,-1213.90,-1115.50))PlayerTextDrawSetString(playerid,Galaxy3StartMenu2,"E");//Whetstone
		SelectTextDraw(playerid,0x33AA33AA);
		return 1;
}

CMD:mynumber(playerid,params[])
{
	    new str[64];
	    format(str,sizeof(str),"Your phone number is {FF9999}%d",MobilePlayer[playerid][number]);
	    SendClientMessage(playerid,-1,str);
	    return 1;
}
	
CMD:number(playerid,params[])
{
		new targetid;
		if(sscanf(params, "u", targetid)) return SendClientMessage(playerid, RED, "[USAGE]: /number [name/id]");
		{
			if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid, RED, "Player not found.");
			{
				new str[64], pName[24];
                GetPlayerName(targetid, (pName), sizeof(pName));
                format(str,sizeof(str),"%s number is {FF9999}%d",pName,MobilePlayer[targetid][number]);
                SendClientMessage(playerid,-1,str);
		    }
	    }
		return 1;
}
	
CMD:phone(playerid,params[])
{
	    if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, RED, "Shoma nemitavanid dar vasile naghlie az mobile estefade konid!");
		if(!GetPVarInt(playerid,"phones3")) GalaxyS3Phone(playerid,1);
		else GalaxyS3Phone(playerid,0);
		return 1;
}
	
CMD:answer(playerid,params[])
{
		if(MobilePlayer[playerid][calling] == 2)
		{
		    KillTimer(TimerWaitCalling[MobilePlayer[playerid][caller]]);
            MobilePlayer[playerid][calling] = 1;
	        MobilePlayer[MobilePlayer[playerid][caller]][calling] = 1;
	        SendClientMessage(playerid, 0xFFFF00AA, "Call started!");
	        SendClientMessage(MobilePlayer[playerid][caller], 0xFFFF00AA, "Call started!");
		}
		return 1;
}

CMD:reject(playerid,params[])
{
		if(MobilePlayer[playerid][calling] == 1)
		{
		    KillTimer(TimerWaitCalling[MobilePlayer[playerid][caller]]);
		    KillTimer(TimerWaitCalling[playerid]);
	        SendClientMessage(playerid, 0xFFFF00AA, "Hung up the call!");
	        SendClientMessage(MobilePlayer[playerid][caller], 0xFFFF00AA, "Hang!");
  			MobilePlayer[MobilePlayer[playerid][caller]][calling] = 0;
            MobilePlayer[MobilePlayer[playerid][caller]][caller] = -1;
  			MobilePlayer[playerid][calling] = 0;
            MobilePlayer[playerid][caller] = -1;
		}
		else if(MobilePlayer[playerid][calling] == 2)
		{
		    KillTimer(TimerWaitCalling[playerid]);
  			KillTimer(TimerWaitCalling[MobilePlayer[playerid][caller]]);
	        SendClientMessage(playerid, 0xFFFF00AA, "Reject the call!");
	        SendClientMessage(MobilePlayer[playerid][caller], 0xFFFF00AA, "Caught the call...");
  			MobilePlayer[MobilePlayer[playerid][caller]][calling] = 0;
            MobilePlayer[MobilePlayer[playerid][caller]][caller] = -1;
  			MobilePlayer[playerid][calling] = 0;
            MobilePlayer[playerid][caller] = -1;
		}
		return 1;
}

