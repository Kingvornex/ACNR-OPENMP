	GetSetObjectsCount[playerid]++;
	GetSetObjects[playerid][GetSetObjectsCount[playerid]] = ObjectInfo[playerid][oCurrent];
	IsPlayerEditingObject{playerid} = false;
	ObjectInfo[playerid][oCurrent] = -1;
