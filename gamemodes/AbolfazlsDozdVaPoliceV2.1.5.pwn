        	SendClientMessage(playerid, RED, fstr2);
        	format(fstr2, sizeof(fstr2), "The suspect has payed the ticket, you got paid $%d.", fine);
        	SendClientMessage(TicketIssuer[playerid], YELLOW, fstr2);
        	GivePlayerCash(playerid, -fine);
