CMD:uncuff(playerid, params[])
{
    new targetid;
    if(sscanf(params, "u", targetid)) return SendClientMessage(playerid,RED, "[USAGE]: /uncuff [name/id]");
	if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid, RED, "Player not found.");
